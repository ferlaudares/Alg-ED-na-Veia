#include <stdio.h>

int main() {
    int matriz[2][2];

    for (int f = 0; f < 2; f++) {
        for (int j = 0; j < 2; j++) {
            printf("Digite o elemento [%d][%d]: ", f, j);
            scanf("%d", &matriz[f][j]);
        }
    }

    printf("A matriz Ã©:\n");
    for (int f = 0; f < 2; f++) {
        for (int j = 0; j < 2; j++) {
            printf("%d ", matriz[f][j]);
        }
        printf("\n");
    }

    return 0;
}
